@model AboutUs
@inject LanguageService language
<h1>@language.Getkey("AboutDetails")</h1>
<p><strong>@language.Getkey("GeneralDescription"):</strong> @Html.Raw(Model.GeneralDescription)</p>
<p><strong>@language.Getkey("TopDescription"):</strong> @Html.Raw(Model.TopDescription)</p>
<p><strong>@language.Getkey("MiddleDescription"):</strong> @Html.Raw(Model.MiddleDescription)</p>
<p><strong>@language.Getkey("BottomDescription"):</strong> @Html.Raw(Model.BottomDescription)</p>
<br />
<a asp-action="Index" class="btn btn-secondary">@language.Getkey("GoToBack")</a>